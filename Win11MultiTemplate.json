{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "imageTemplateName": {
        "type": "string"
      },
      "api-version": {
        "type": "string"
      },
      "svclocation": {
        "type": "string"
      }
    },
    "variables": {},
    "resources": [
      {
        "name": "[parameters('imageTemplateName')]",
        "type": "Microsoft.VirtualMachineImages/imageTemplates",
        "apiVersion": "[parameters('api-version')]",
        "location": "[parameters('svclocation')]",
        "dependsOn": [],
        "tags": {
          "imagebuilderTemplate": "win11multi",
          "userIdentity": "enabled"
        },
        "identity": {
          "type": "UserAssigned",
          "userAssignedIdentities": {
            "/subscriptions/b147624b-199d-44dc-98ff-3953b00c8287/resourcegroups/SivaG-RG/providers/Microsoft.ManagedIdentity/userAssignedIdentities/AVD-Managed-identity": {}
          }
        },
        "properties": {
          "buildTimeoutInMinutes": 90,
          "vmProfile": {
            "vmSize": "Standard_D2s_v3",
            "osDiskSizeGB": 127
          },
          "source": {
            "type": "PlatformImage",
            "publisher": "microsoftwindowsdesktop",
            "offer": "windows-11",
            "sku": "win11-21h2-avd",
            "version": "latest"
          },
          "customize": [
            
            {
              "type": "PowerShell",
              "runElevated": true,
              "name": "RunPoShInstall",
              "scriptUri": "https://avdpipe.blob.core.windows.net/script/install-software.ps1?sp=r&st=2023-07-28T06:30:32Z&se=2023-08-28T14:30:32Z&spr=https&sv=2022-11-02&sr=b&sig=2jHo%2Bk9sZOAtDQyU6xXdsfcMZDTy7RYjeaiPvCvuG2Y%3D"
            }
          ],
          "distribute": [
            {
              "type": "SharedImage",
              "galleryImageId": "/subscriptions/b147624b-199d-44dc-98ff-3953b00c8287/resourceGroups/SivaG-RG/providers/Microsoft.Compute/galleries/avdgallery/images/avd",
              "runOutputName": "win11Client",
              "artifactTags": {
                "Owner": "Image Builder"
              },
              "replicationRegions": [
              ]
            }
          ]
        }
      }
    ]
  }
  
